{
  "root": true,
  "env":{
    "browser": true,  
    "node": true,  
    "commonjs": true ,
    "amd": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "plugins": [
    "vue"
  ],
  "extends":"vue",
  "rules": {
    "no-inner-declarations":"warn",
    "new-cap":"off",
    "one-var": 0,
    "camelcase": 0,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-prototype-builtins":"off",
    "object-curly-spacing": ["error", "always"],
    "no-multiple-empty-lines": ["error", { "max": 2, "maxBOF": 1 }]
  }
}
